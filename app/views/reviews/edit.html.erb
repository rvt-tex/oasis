<h1>Edit Review!</h1>

<%= render partial: "layouts/errors", locals: {object: @review} %>

<%= form_for(@review) do |f|%>

<br><br>
  <%= f.label :appointment %>
  <%= f.collection_select :appointment_id, Appointment.all, :id, :desired_date %>
<br><br>
  <%= f.label :comment %>
  <%= f.text_area :comment %>
  <br><br>
  <%= f.submit %>

<% end %>